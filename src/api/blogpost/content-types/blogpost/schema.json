{
  "kind": "collectionType",
  "collectionName": "blogposts",
  "info": {
    "singularName": "blogpost",
    "pluralName": "blogposts",
    "displayName": "blogpost"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "blog_post_title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "blog_post_description": {
      "type": "text",
      "required": true
    },
    "blog_post_content": {
      "type": "blocks",
      "required": true
    },
    "blog_post_image_url": {
      "type": "text",
      "required": true,
      "unique": false
    },
    "domains": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::domain.domain",
      "mappedBy": "blogposts"
    },
    "blog_post_status": {
      "type": "enumeration",
      "required": true,
      "default": "save",
      "enum": [
        "save",
        "draft",
        "publish"
      ]
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category",
      "mappedBy": "blogpost"
    },
    "likes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::like.like",
      "inversedBy": "blogposts"
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment",
      "mappedBy": "blogpost"
    },
    "bookmarks": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::bookmark.bookmark",
      "mappedBy": "blogposts"
    }
  }
}
