{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "category"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "category_name": {
      "type": "string",
      "required": true
    },
    "category_url": {
      "type": "text",
      "required": true,
      "unique": true
    },
    "category_image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "domains": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::domain.domain",
      "mappedBy": "category"
    }
  }
}
